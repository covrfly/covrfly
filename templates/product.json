           minDate: minDeliveryDate.toLocaleDateString('es-MX', options),\n            maxDate: maxDeliveryDate.toLocaleDateString('es-MX', options),\n        };\n    }\n\n    function updateTime() {\n        const timeLeft = calculateTimeLeft();\n\n        if (document.getElementById('time-left')) {\n            document.getElementById('time-left').innerHTML = `\n                <strong>${timeLeft.hours} hrs ${timeLeft.minutes} min<\/strong>`;\n        }\n    }\n\n    const standardDates = calculateDeliveryDays(standardDelivery);\n    const expressDates = calculateDeliveryDays(expressDelivery);\n\n    window.onload = function () {\n        if (document.getElementById('delivery-standard')) {\n            document.getElementById('delivery-standard').innerHTML = `\n                ${standardDates.minDate} - ${standardDates.maxDate}`;\n        }\n        if (document.getElementById('delivery-express')) {\n            document.getElementById('delivery-express').innerHTML = `\n                ${expressDates.minDate} - ${expressDates.maxDate}`;\n        }\n\n        updateTime();\n        setInterval(updateTime, 1000);\n    };\n<\/script>\n\n<div class=\"delivery-container\">\n    <p class=\"delivery-title\">Haz tu pedido en las próximas<\/p>\n    <p id=\"time-left\" class=\"delivery-time\"><\/p>\n    \n    <div class=\"delivery-item\">\n        <div class=\"delivery-header\">\n            <span class=\"delivery-icon\">📦<\/span>\n            <p class=\"delivery-method\">Entrega Estándar<\/p>\n        <\/div>\n        <p id=\"delivery-standard\" class=\"delivery-date\"><\/p>\n    <\/div>\n\n    <div class=\"delivery-item\">\n        <div class=\"delivery-header\">\n            <span class=\"delivery-icon\">⚡<\/span>\n            <p class=\"delivery-method\">Entrega Express<\/p>\n        <\/div>\n        <p id=\"delivery-express\" class=\"delivery-date\"><\/p>\n    <\/div>\n<\/div>"}}},"block_order":["custom_liquid_hTXRMb","variant_picker","title","price","buy_buttons","f73